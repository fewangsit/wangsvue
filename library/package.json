{
  "name": "wangsvue",
  "version": "1.4.14",
  "author": "fewangsit",
  "description": "Wangsit VueJS Component Library",
  "type": "module",
  "module": "./dist/wangsvue.es.js",
  "types": "./dist/main.d.ts",
  "scripts": {
    "build": "rimraf dist && vue-tsc && vite build --config ./vite.config.ts && ./scripts/postbuild.sh",
    "release": "pnpm build && changelogen --release && ./scripts/publish.sh",
    "release:patch": "pnpm build && changelogen --release --patch && ./scripts/publish.sh"
  },
  "exports": {
    ".": "./dist/wangsvue.es.js",
    "./colors.config.json": "./colors.config.json"
  }
}